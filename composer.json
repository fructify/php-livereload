{
    "name": "fructify/reload",
    "description": "A livereload server implemented in php.",
    "homepage": "https://github.com/fructify/reload",
    "keywords": ["live", "reload", "watch"],
    "license": "MIT",
    "authors":
    [
        {
            "name": "Ricky Su",
            "email": "ricky@ez2.us"
        },{
            "name": "Brad Jones",
            "email": "brad@bjc.id.au"
        }
    ],
    "autoload":
    {
        "psr-4":
        {
            "Fructify\\Reload\\": "src"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Fructify\\Reload\\Tests\\": "tests"
        }
    },
    "require":
    {
        "php": "^7.0",
        "react/socket": "^0.4.0",
        "symfony/finder": "^2.6||^3.0",
        "symfony/console": "^2.6||^3.0",
        "symfony/http-foundation": "^2.6||^3.0"
    },
    "require-dev":
    {
        "phpunit/phpunit": "^6.0",
        "consolidation/robo": "^1.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "bin": ["bin/reload"]
}
